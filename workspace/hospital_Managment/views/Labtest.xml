<odoo>
	#Appointment Tree view
	
	<record id="view_labtest_tree" model="ir.ui.view">

		<field name="name">lebtest.tree</field>
		<field name="model">hospital.labtest</field>
		<field name="arch" type="xml">
			<tree>

				<field name="test_name"></field>
				<field name="price"></field>
				
			</tree>
			
		</field>

	</record>
	

	<record id="view_labtest_form" model="ir.ui.view">

		<field name="name">lebtest.form</field>
		<field name="model">hospital.labtest</field>
		<field name="arch" type="xml">
			<tree>

				<field name="test_name"></field>
				<field name="price"></field>
				
			</tree>
			
		</field>

	</record>

</odoo>